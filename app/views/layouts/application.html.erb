  <body>

    <div class="navbar navbar-inverse navbar-fixed-top">
    
      <div class="navbar-inner">
      <div class="container">
        	<a class="brand" href="/">Strategy Stroller</a>
        					<ul class = "nav">
<li id ="navbar_forms"><%= link_to "Forms", forms_path %></li>
<li id="navbar_reports"><a>Reports</a></li>
<li id= "navbar_setup"><%= link_to "Setup", input_framework_path %></li>
<li id= "navbar_edit_users"><%= link_to "Edit Users", edit_users_path %></li>
			<div class="btn-group pull-right">


        	</ul>
			  <a class="btn dropdown-toggle btn-inverse pull-right" data-toggle="dropdown">
			    User Settings
			    <span class="caret"></span>
			  </a>
			  <ul class="dropdown-menu">
			  	<li><%= render 'devise/menu/login_items' %></li>
			    <li><%= render 'devise/menu/registration_items' %></li>
			  </ul>
			</div>
            </div>
          
      </div>
    </div>

    <div class="container body_container">

      <!-- Display messages For devise. -->
      <p class="notice"><%= notice %></p>
      <% if alert %>
      <p class="alert"><%= alert %></p>
      <% end %>
      <%= yield %>

    </div> <!-- /container -->

